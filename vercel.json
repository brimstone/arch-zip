{
  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index"
    },
    {
      "source": "/:path*",
      "destination": "/api/index"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://download.mozilla.org/?product=firefox-latest&os=win64&lang=en-US",
      "has": [
        {
          "type": "host",
          "value": "firefox.x86-64.zip"
        }
      ]
    },
    {
      "source": "/",
      "destination": "https://download.mozilla.org/?product=firefox-latest&os=win&lang=en-US",
      "has": [
        {
          "type": "host",
          "value": "firefox.i386.zip"
        }
      ]
    }
  ]
}
